    public static final TalonSRX drive1 =new TalonSRX();//there are 2 moters hooked up to the current drive train
    public static final TalonSRX drive2 =new TalonSRX();

public void driveForword(){

void frc.robot.subsystems.drive.drive.initialize();

}

boolean driveForwardBoolean;

@Override
public void initialize(){
    super.initialize();
driveForwardBoolean=true;
}

@Overide
public void execute(){
if (driveForwardBoolean = true){
    //move forward stuff
}

if (driveForwardBoolean = false){
    //no move forward stuff
}
}


@Override
public boolean isfinished(){

}

@Overide
public void end(boolean interrupted){
driveForwardBoolean=false;
}

}
